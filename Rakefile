task default: :clean

desc "Clean Up LaTeX Build Files"
task :clean do
  `rm -f main-blx.bib`
  `rm -f main.aux`
  `rm -f main.bbl`
  `rm -f main.bcf`
  `rm -f main.blg`
  `rm -f main.fdb_latexmk`
  `rm -f main.fls`
  `rm -f main.log`
  `rm -f main.nav`
  `rm -f main.out`
  `rm -f main.pdf`
  `rm -f main.run.xml`
  `rm -f main.snm`
  `rm -f main.synctex.gz`
  `rm -f main.synctex.gz*`
  `rm -f main.toc`
end